{
    "manifest_version": 3,
    "name": "AlibabaPuHuiTi Text Collector",
    "version": "1.0",
    "description": "Collects unique characters using AlibabaPuHuiTi font in 1 minute",
    "permissions": [
      "activeTab",
      "tabs",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "default_icon.png"
    },
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/content.js"]
    }
  ],
  "background": {
    "service_worker": "scripts/background.js"
  }
  }